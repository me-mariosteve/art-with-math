*.gitignore filter=lfs diff=lfs merge=lfs -text
*.txt filter=lfs diff=lfs merge=lfs -text
*.md filter=lfs diff=lfs merge=lfs -text
*.sh filter=lfs diff=lfs merge=lfs -text
/cppcheck filter=lfs diff=lfs merge=lfs -text
*.avi filter=lfs diff=lfs merge=lfs -text
*.cppcheck filter=lfs diff=lfs merge=lfs -text
*.cpp filter=lfs diff=lfs merge=lfs -text
*.hpp filter=lfs diff=lfs merge=lfs -text
*.cpp !text !filter !merge !diff
*.hpp !text !filter !merge !diff
* !text !filter !merge !diff
out.avi filter=lfs diff=lfs merge=lfs -text
out.avi !text !filter !merge !diff
